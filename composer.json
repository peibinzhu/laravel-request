{
  "name": "peibin/laravel-request",
  "description": "Peibin request component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "laravel"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-context": "^2.0",
    "peibin/laravel-swoole-event": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Request\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Request\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Request\\RequestServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
